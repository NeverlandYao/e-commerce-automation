{
  "name": "crawler-sidecar",
  "version": "1.0.0",
  "description": "Node.js sidecar process for e-commerce crawler",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "dev": "NODE_ENV=development node src/main.js",
    "build": "node build.js",
    "build:all": "npm run build:macos && npm run build:windows && npm run build:linux",
    "build:macos": "pkg src/main.js --targets node18-macos-x64 --output bin/crawler-sidecar-x86_64-apple-darwin",
    "build:windows": "pkg src/main.js --targets node18-win-x64 --output bin/crawler-sidecar-x86_64-pc-windows-msvc.exe",
    "build:linux": "pkg src/main.js --targets node18-linux-x64 --output bin/crawler-sidecar-x86_64-unknown-linux-gnu",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf bin/*"
  },
  "keywords": [
    "crawler",
    "sidecar",
    "tauri",
    "e-commerce",
    "scraping"
  ],
  "author": "E-commerce AI Team",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.40.0",
    "crawlee": "^3.5.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.0",
    "user-agents": "^1.0.1235",
    "proxy-agent": "^6.3.1"
  },
  "devDependencies": {
    "pkg": "^5.8.1",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pkg": {
    "assets": [
      "node_modules/playwright-core/**/*",
      "node_modules/playwright/**/*"
    ],
    "scripts": [
      "src/**/*.js"
    ],
    "targets": [
      "node18-macos-x64",
      "node18-win-x64",
      "node18-linux-x64"
    ],
    "outputPath": "bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/e-commerce-ai"
  },
  "bugs": {
    "url": "https://github.com/your-org/e-commerce-ai/issues"
  },
  "homepage": "https://github.com/your-org/e-commerce-ai#readme"
}